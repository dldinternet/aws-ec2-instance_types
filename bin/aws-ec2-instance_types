#!/usr/bin/env ruby

# Adjust lib path
_lib=File.expand_path(File.dirname(__FILE__) + '/../lib')
$:.unshift(_lib) unless $:.include?(_lib)
require 'dldinternet/aws/ec2/instance_types/cli'

# =====================================================================================================================
rc = DLDInternet::AWS::EC2::Instance_Types::Cli.start(ARGV)
if rc.is_a?(Fixnum)
  exit rc
else
  puts rc.ai
  exit 0
end

